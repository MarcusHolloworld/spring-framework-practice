package com.springcore.lifecycle;

public class Samosa {
	private double price;
	
	public Samosa(double price) {
		this.price = price;
	}
	
	public void setPrice(double price) {
		this.price = price;
	}
	
	public double srtPrice() {
		return price;
	}

	@Override
	public String toString() {
		return "Samosa [price=" + price + "]";
	}
	
	
}
